{
  "name": "@glmachado/any-store",
  "private": false,
  "version": "0.4.0",
  "type": "module",
  "files": [
    "dist"
  ],
  "module": "./dist/lib.js",
  "types": "./dist/src/lib.d.ts",
  "exports": {
    ".": {
      "import": "./dist/lib.es.js",
      "types": "./dist/src/lib.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "build": "npm run check && vite build",
    "preview": "vite preview",
    "check": "tsc --build",
    "test": "vitest",
    "bench": "vitest bench",
    "test:browser": "vitest --config=vitest.browser.config.ts",
    "test-ci": "cargo wasm && vitest --run & vitest --config=vitest.browser.config.ts --run --browser.headless & cargo test",
    "deploy": "npm run build && npm run test-ci && npm publish --access public"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@vitest/browser-playwright": "^4.0.18",
    "tsx": "^4.21.0",
    "typescript": "~5.9.3",
    "vite": "npm:rolldown-vite@7.2.5",
    "vite-plugin-dts": "^4.5.4",
    "@glmachado/any-store": "^0.1.1",
    "vitest": "^4.0.16"
  },
  "overrides": {
    "vite": "npm:rolldown-vite@7.2.5"
  },
  "dependencies": {}
}
